@model CT.Common.Entities.DashEntity
@{
    var deals = Model.dashEntities.Where(x => x.rowtype == "deals").ToList();
}

<table class="table table-hover">
    <thead class="text-warning">
    <th>Name</th>
    <th>Mobile</th>
    <th>Car Name</th>
    <th>Date</th>
    <th>Bid Amount</th>
    </thead>
    <tbody>
        @foreach (var item in deals)
        {
        <tr>
            <td>@item.c1</td>
            <td>@item.c2</td>
            <td>@item.c3</td>
            <td>@item.c5</td>
            <td>@item.c4</td>
        </tr>
        }
    </tbody>
</table>
